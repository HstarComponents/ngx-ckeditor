{
  "name": "ngx-ckeditor",
  "version": "0.1.1",
  "description": "The CKEditor component for angular(2.x and 4.x)",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "cleanlib": "node build/cleanlib.js",
    "publish": "npm run cleanlib && ngc -p tsconfig-aot.json",
    "build": "webpack --config build/webpack.prod.conf.js",
    "build:aot": "npm run cleanlib && ngc -p tsconfig-demo-aot.json && webpack --config build/webpack.aot.conf.js",
    "test": "echo \"ok\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HstarComponents/ngx-ckeditor.git"
  },
  "keywords": [
    "Ckeditor",
    "Component",
    "Angular",
    "Ng2"
  ],
  "author": "Jay.M.Hu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HstarComponents/ngx-ckeditor/issues"
  },
  "homepage": "https://github.com/HstarComponents/ngx-ckeditor#readme",
  "devDependencies": {
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@types/node": "^8.0.13",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.2",
    "shelljs": "^0.7.8",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.13"
  },
  "dependencies": {}
}
